{
  "name": "teste",
  "auto-changelog": {
    "replaceText": {
      "(\\w+-\\d+)": "[`$1`](https://issues.apache.org/jira/browse/$1)",
      "\\):": "",
      "feat: ": "",
      "feat\\(": "",
      "fix: ": "",
      "fix\\(": "",
      "hotfix: ": "",
      "hotfix\\(": ""
    }
  },
  "version": "1.3.1",
  "scripts": {
    "version": "auto-changelog -p --template changelog-template.hbs && git add CHANGELOG.md"
  },
  "dependencies": {
    "auto-changelog": "^2.4.0"
  }
}
